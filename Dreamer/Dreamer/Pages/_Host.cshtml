@page "/auth/login"
@inject Blazored.LocalStorage.ILocalStorageService sessionStorage
@namespace Dreamer.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Blazor-POS</title>
    <base href="~/" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:300,400,400i,600,700,800,900" rel="stylesheet" />
    <link rel="stylesheet" href="_content/Radzen.Blazor/css/default.css" />
    <link href="dist-assets/css/themes/lite-purple.css" rel="stylesheet" />
    <link href="dist-assets/css/plugins/perfect-scrollbar.css" rel="stylesheet" />
    <link rel="stylesheet" href="dist-assets/css/plugins/fontawesome-5.css" />
    <link href="dist-assets/css/plugins/metisMenu.min.css" rel="stylesheet" />
    <link href="css/site.css" rel="stylesheet" />

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
<link href="_content/MudBlazor/MudBlazor.min.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />

</head>
<body>
    <component type="typeof(App)" render-mode="Server" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    <!-- ============ Search UI End ============= -->
    <script src="dist-assets/js/plugins/jquery-3.3.1.min.js"></script>
    <script src="dist-assets/js/plugins/bootstrap.bundle.min.js"></script>
    <script src="_framework/blazor.server.js"></script>
    <script src="_content/Radzen.Blazor/Radzen.Blazor.js"></script>
    <script src="_content/MudBlazor/MudBlazor.min.js"></script>
    <script src="_content/BlazorInputFile/inputfile.js"></script>
     <script src="js/JSInterop.js"></script>
     <script src="js/SaveAsPdf.js"></script>
     <script src="js/jsPrint.js"></script>
    <script type="text/javascript">

        function printDiv() {
     var printwin = window.open("");
  printwin.document.write(document.getElementById("PrintDiv").innerHTML);
  printwin.stop();
  printwin.print();
  printwin.close();
        }
    </script>
    <script>
   Blazor.defaultReconnectionHandler._reconnectCallback = function(d) {
        document.location.reload(); 
   }
</script>
</body>

</html>
